<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
 
    <context:annotation-config />
	<!-- The autodetect option was deprecated as of Spring 3.x -->
	<bean id="employeeBean" class="com.osp.autowireByName.EmployeeBean" autowire="autodetect">
       	<property name="fullName" value="Muthukumar Thangavinayagam" /> <!-- Property Initialization -->
    </bean>
  
    <bean id="department" class="com.osp.autowireByName.DepartmentBean" >
   		<property name="name" value="Computer Science"/>
    </bean>
 
 	<bean id="triangleBean" class="com.osp.applicationcontext.example.Triangle">
		<!-- setter injection. -->
		<property name="type" value="Equalateral" /> <!-- Property Initialization -->
	</bean>
 
</beans>